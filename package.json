{
  "name": "skupper-web-console",
  "description": "Skupper Web Console",
  "license": "Apache-2.0",
  "keywords": [
    "skupper",
    "console",
    "monitoring",
    "observability",
    "connectivity",
    "openshift"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skupperproject/skupper-console.git"
  },
  "bugs": {
    "url": "https://github.com/skupperproject/skupper-console/issues"
  },
  "private": true,
  "scripts": {
    "start": "webpack serve --config config/webpack.dev.js",
    "build": "webpack --config config/webpack.prod.js",
    "test": "jest --config config/jest.config.js",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "ci": "start-test 3000 cy:run",
    "lint": "eslint src --ext .ts,.tsx --cache",
    "lint-fix": "yarn lint --fix",
    "format": "prettier --write 'src/**/*.{js,ts,tsx,json,css}'",
    "bundle-report": "STATS=server yarn build",
    "find-deadcode": "ts-prune",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "dependencies": {
    "@patternfly/patternfly": "^4.224.2",
    "@patternfly/react-charts": "^6.94.18",
    "@patternfly/react-core": "^4.276.6",
    "@patternfly/react-icons": "^4.93.6",
    "@patternfly/react-table": "^4.112.39",
    "@tanstack/react-query": "^4.26.1",
    "axios": "^1.3.4",
    "d3-drag": "^3.0.0",
    "d3-ease": "^3.0.1",
    "d3-fetch": "^3.0.1",
    "d3-force": "^3.0.0",
    "d3-interpolate": "^3.0.1",
    "d3-polygon": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-zoom": "^3.0.0",
    "framer-motion": "^10.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/d3-drag": "^3.0.2",
    "@types/d3-force": "^3.0.4",
    "@types/d3-polygon": "^3.0.0",
    "@types/d3-selection": "^3.0.4",
    "@types/d3-transition": "^3.0.3",
    "@types/d3-zoom": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "circular-dependency-plugin": "^5.2.2",
    "commitizen": "^4.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "cypress": "^12.8.0",
    "cz-git": "^1.6.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "miragejs": "^0.1.47",
    "prettier": "^2.8.4",
    "start-server-and-test": "^2.0.0",
    "style-loader": "^3.3.2",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "ts-prune": "^0.10.3",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^4.9.5",
    "webpack": "^5.76.1",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.12.0",
    "webpack-merge": "^5.8.0"
  },
  "engines": {
    "node": ">=16.17.1",
    "npm": ">=6.0.0 ",
    "yarn": ">= 1.19.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "browserslist": [
    ">10%",
    "last 2 versions",
    "not ie <= 11"
  ]
}
