{
    "name": "skupper-web-console",
    "description": "Skupper Web Console",
    "license": "Apache-2.0",
    "private": true,
    "engines": {
        "npm": "please-use-yarn",
        "yarn": ">= 1.19.1"
    },
    "scripts": {
        "start": "webpack serve --config config/webpack.dev.js",
        "build": "webpack --config config/webpack.prod.js",
        "test": "jest --config config/jest.config.js",
        "cy:open": "cypress open",
        "cy:run": "cypress run",
        "ci": "start-test 3000 cy:run",
        "lint": "eslint src --ext .ts,.tsx --cache",
        "lint-fix": "yarn lint --fix",
        "prettier-all": "prettier --write '**/*.{js,ts,tsx,html,scss}'",
        "bundle-report": "STATS=server yarn build",
        "find-deadcode": "ts-prune",
        "prepare": "husky install"
    },
    "dependencies": {
        "@patternfly/patternfly": "^4.224.2",
        "@patternfly/react-charts": "^6.94.18",
        "@patternfly/react-core": "^4.276.6",
        "@patternfly/react-icons": "^4.93.6",
        "@patternfly/react-table": "^4.112.39",
        "@tanstack/react-query": "^4.24.4",
        "axios": "^1.3.2",
        "d3-collection": "^1.0.7",
        "d3-drag": "^3.0.0",
        "d3-ease": "^3.0.1",
        "d3-fetch": "^3.0.1",
        "d3-force": "^3.0.0",
        "d3-interpolate": "^3.0.1",
        "d3-polygon": "^3.0.1",
        "d3-scale": "^4.0.2",
        "d3-selection": "^3.0.0",
        "d3-shape": "^3.2.0",
        "d3-zoom": "^3.0.0",
        "framer-motion": "^9.0.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.8.1"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/d3-collection": "^1.0.10",
        "@types/d3-drag": "^3.0.2",
        "@types/d3-force": "^3.0.4",
        "@types/d3-polygon": "^3.0.0",
        "@types/d3-selection": "^3.0.4",
        "@types/d3-transition": "^3.0.3",
        "@types/d3-zoom": "^3.0.2",
        "@typescript-eslint/eslint-plugin": "^5.51.0",
        "@typescript-eslint/parser": "^5.51.0",
        "circular-dependency-plugin": "^5.2.2",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.7.3",
        "cypress": "^12.5.1",
        "eslint": "^8.33.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-import-resolver-typescript": "^3.5.3",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "fork-ts-checker-webpack-plugin": "^7.3.0",
        "html-webpack-plugin": "^5.5.0",
        "husky": "^8.0.3",
        "jest": "^29.4.2",
        "jest-environment-jsdom": "^29.4.2",
        "miragejs": "^0.1.47",
        "prettier": "^2.8.3",
        "start-server-and-test": "^1.15.3",
        "style-loader": "^3.3.1",
        "ts-jest": "^29.0.5",
        "ts-loader": "^9.4.2",
        "ts-node": "^10.9.1",
        "ts-prune": "^0.10.3",
        "tsconfig-paths-webpack-plugin": "^4.0.0",
        "typescript": "^4.9.5",
        "webpack": "^5.75.0",
        "webpack-bundle-analyzer": "^4.7.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.11.1",
        "webpack-merge": "^5.8.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
